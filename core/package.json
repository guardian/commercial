{
	"name": "@guardian/commercial-core",
	"version": "29.1.0",
	"description": "Guardian advertising business logic",
	"homepage": "https://github.com/guardian/commercial#readme",
	"bugs": {
		"url": "https://github.com/guardian/commercial/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/guardian/commercial.git"
	},
	"license": "Apache-2.0",
	"files": [
		"dist"
	],
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"exports": {
		".": {
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js",
			"workspace": "./src/index.ts"
		},
		"./*": {
			"import": "./dist/esm/*.js",
			"require": "./dist/cjs/*.js",
			"workspace": "./src/*.ts"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "pnpm run --parallel --aggregate-output /^build:.*/",
		"clean": "rm -rf dist",
		"build:esm": "tsc --outDir ./dist/esm --project ./tsconfig.esm.json",
		"build:cjs": "tsc --outDir ./dist/cjs --project ./tsconfig.cjs.json",
		"lint": "eslint .",
		"prettier:check": "prettier . --check --cache",
		"prettier:fix": "prettier . --write --cache",
		"test": "jest",
		"tsc": "tsc --noEmit"
	},
	"peerDependencies": {
		"@guardian/ab-core": "^8.0.1",
		"@guardian/libs": "^26.0.0"
	},
	"dependencies": {
		"@guardian/ab-core": "8.0.1",
		"@guardian/libs": "26.0.0",
		"@types/googletag": "~3.3.0"
	},
	"devDependencies": {
		"@guardian/ophan-tracker-js": "2.6.1",
		"@types/jest": "30.0.0",
		"@types/node": "24.7.2",
		"jest": "^30.2.0",
		"jest-environment-jsdom": "^30.2.0",
		"jest-environment-jsdom-global": "~4.0.0",
		"ts-jest": "^29.4.4",
		"typescript": "5.9.3",
		"type-fest": "^4.41.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"packageManager": "pnpm@9.15.2"
}
