{
	"name": "@guardian/commercial-bundle",
	"version": "0.0.0",
	"description": "Guardian advertising business logic",
	"homepage": "https://github.com/guardian/commercial#readme",
	"bugs": {
		"url": "https://github.com/guardian/commercial/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/guardian/commercial.git"
	},
	"private": true,
	"license": "Apache-2.0",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "pnpm run --parallel --aggregate-output /^build:.*/",
		"build:dev": "webpack -c webpack.config.dev.mjs",
		"build:prod": "webpack -c webpack.config.prod.mjs",
		"clean": "rm -rf dist",
		"playwright:run": "playwright test",
		"playwright:open": "playwright test --ui",
		"lint": "eslint .",
		"lint-staged": "lint-staged",
		"prepare": "husky",
		"prettier:check": "prettier . --check --cache",
		"prettier:fix": "prettier . --write --cache",
		"serve": "OVERRIDE_BUNDLE=true pnpm webpack-dev-server -c ./webpack.config.dev.mjs",
		"test": "jest",
		"tsc": "tsc --noEmit",
		"validate": "npm-run-all tsc lint test build"
	},
	"dependencies": {
		"@guardian/ab-core": "8.0.1",
		"@guardian/core-web-vitals": "15.0.0",
		"@guardian/commercial-core": "workspace:*",
		"@guardian/identity-auth": "^11.0.0",
		"@guardian/identity-auth-frontend": "13.0.0",
		"@guardian/libs": "26.0.0",
		"@guardian/source": "11.3.0",
		"fastdom": "^1.0.12",
		"lodash-es": "^4.17.21",
		"prebid.js": "9.46.0",
		"process": "^0.11.10",
		"web-vitals": "^5.1.0"
	},
	"devDependencies": {
		"@guardian/ophan-tracker-js": "2.6.1",
		"@babel/cli": "^7.28.3",
		"@babel/core": "^7.28.4",
		"@babel/plugin-syntax-dynamic-import": "^7.8.3",
		"@babel/plugin-transform-runtime": "^7.28.3",
		"@babel/preset-env": "^7.28.3",
		"@babel/preset-react": "^7.26.3",
		"@changesets/cli": "^2.29.7",
		"@guardian/browserslist-config": "^6.1.1",
		"@playwright/test": "1.56.0",
		"@types/jest": "30.0.0",
		"@types/lodash-es": "^4.17.12",
		"@types/node": "24.7.2",
		"@types/webpack-env": "^1.18.5",
		"@types/googletag": "~3.3.0",
		"babel-jest": "^30.2.0",
		"babel-loader": "^10.0.0",
		"babel-plugin-dynamic-import-node": "^2.3.3",
		"circular-dependency-plugin": "^5.2.2",
		"globals": "16.4.0",
		"html-webpack-plugin": "5.6.4",
		"husky": "^9.1.7",
		"jest": "^30.2.0",
		"jest-environment-jsdom": "^30.2.0",
		"jest-environment-jsdom-global": "~4.0.0",
		"lint-staged": "^16.2.5",
		"npm-run-all": "^4.1.5",
		"raw-loader": "^4.0.2",
		"terser-webpack-plugin": "^5.3.11",
		"ts-jest": "^29.4.5",
		"ts-loader": "^9.5.4",
		"type-fest": "^4.41.0",
		"typescript": "5.9.3",
		"webpack": "^5.102.1",
		"webpack-bundle-analyzer": "^4.10.2",
		"webpack-cli": "^6.0.1",
		"webpack-dev-server": "^5.2.0",
		"webpack-merge": "^6.0.1"
	},
	"prettier": "@guardian/prettier",
	"publishConfig": {
		"access": "public"
	},
	"browserslist": [
		"extends @guardian/browserslist-config"
	],
	"packageManager": "pnpm@9.15.2"
}
