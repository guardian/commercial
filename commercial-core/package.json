	},
	"devDependencies": {
		"@types/node": "20.17.24",
		"typescript": "5.5.4"
	},
	"publishConfig": {
		"access": "public"
	},
	"browserslist": [
		"extends @guardian/browserslist-config"
	],
	"packageManager": "pnpm@9.15.2"
